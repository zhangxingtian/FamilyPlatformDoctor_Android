<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="style_main_bottom_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/px_30</item>
    </style>
    <style name="style_vertical_v">
        <item name="android:layout_width">@dimen/px_1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/line_main_color</item>
    </style>
    <style name="style_horizontal_v">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/px_1</item>
        <item name="android:background">@color/line_main_color</item>
    </style>
</resources>